<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Amazon Test" parallel="tests" thread-count="1">

    <listeners>
        <listener class-name="util.Listeners"/>

    </listeners>

    <test name="Search a product">
        <classes>
            <class name="SearchTest"/>
        </classes>
    </test>

    <test name="Verify language site">
        <classes>
            <class name="SiteLanguageTest"/>
        </classes>
    </test>


</suite> <!-- Suite -->
